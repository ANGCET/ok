 function autoskill1()
    while getgenv().autoskill1 == true do
    wait(0.1)
local args = {
    [1] = "Skill",
    [2] = "Skill1",
    [3] = "Attack"
}

game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("attack"):FireServer(unpack(args))
    end
  end

